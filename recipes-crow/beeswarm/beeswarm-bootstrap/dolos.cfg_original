# IP of the OSSEC server
OSSEC_SERVER=192.168.1.1
# Key as generated by OSSEC server, this is a base64 encoded string.
AGENT_KEY=

# STATIC or DHCP
IP_MODE=STATIC

# The options below is only relevant if IP_MODE is STATIC
IP_ADDRESS=192.168.3.1
IP_GATEWAY=192.168.1.1
IP_BROADCAST=192.168.2.255
IP_NETMASK=255.255.255.0
NAMESERVER=8.8.8.8
